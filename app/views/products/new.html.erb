<section>
  <p><%@product.merchant%></p>
  <%= form_for ([@merchant, @product]) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :price %>
      <%= f.text_field :price %>
      <%= f.label :stock %>
      <%= f.text_field :stock %>
      <%= f.label :photo_url %>
      <%= f.text_field :photo_url %>
      <%= f.submit value: "Create new product" %>
    <% end %>
</section>
